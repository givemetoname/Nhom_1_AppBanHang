    <!-- Featured Section Begin -->
    <section class="featured spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>Sản phẩm nổi bật</h2>
                    </div>
                    <div class="featured__controls">
                        <ul>
                            <li class="active" data-filter="*">tất cả đồ ăn</li>
                            <li data-filter=".oranges">Cam</li>
                            <li data-filter=".fresh-meat">thịt</li>
                            <li data-filter=".vegetables">rau</li>
                            <li data-filter=".fastfood">đồ ăn nhanh</li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row featured__filter">
                <% Products.forEach((product)=>  { %> 
                <div class="col-lg-3 col-md-4 col-sm-6 mix oranges fresh-meat">
                    <div class="featured__item">
                        <div class="featured__item__pic set-bg"  data-setbg="">
                            <a href="/pages/productDetails/<%= product._id  %> "><img src="<%= product.image %>" alt=""></a>
                            <ul class="featured__item__pic__hover">
                                <li><a href="#"><i class="fa fa-heart"></i></a></li>
                                <li><a href="#"><i class="fa fa-retweet"></i></a></li>
                                <li><a href="/add-to-cart/<%= product._id  %>"><i class="fa fa-shopping-cart"></i></a></li>
                            </ul>
                        </div>
                        <div class="featured__item__text">
                            <h6><a href="#"><%= product.nameFuitl %> </a></h6>
                            <h5><%= product.price  %></h5>
                        </div>
                    </div>
                </div>
                <% }) %> 
            </div>
            <div class="pagination  justify-content-center">
                <div class="row">
                    <!-- hiển thị phân trang nếu có trang -->
                    <% if(pages > 0) { %>
                    <nav class="mx-auto">
                      <ul class="pagination" style="margin-top: 2rem;">
                        <!-- FIRST ITEM -->  
                        <% if(current == 1) { %> 
                            <li class="page-item disabled">
                              <a class="page-link" href="/">First</a>
                            </li>
                        <% } else { %>
                        
                            <li class="page-item">
                              <a class="page-link" href="/">First</a>
                            </li>
                        <% } %>
                        
                        <!-- ITEMS  -->
                        <% var i = (Number(current) > 3 ? Number(current) - 2 : 1) %> 
            
                        <% if(i !== 1) { %>
                            <li class="page-item disabled">
                              <a class="page-link" href="#">...</a>
                            </li>
                        <% } %>
                        <% for(; i <= (Number(current) + 2) && i <= pages; i++) { %>
                    
                          <% if(i == current) { %>
                            <li class="page-item active">
                              <a class="page-link" href="/home/news/<%= i %>/">
                                <%= i %>
                              </a>
                            </li>
                        <% } else { %>
                            <li class="page-item">
                              <a class="page-link" href="/home/news/<%= i %>/">
                                <%= i %>
                              </a>
                            </li>
                        <% } %>
                        <% if (i == Number(current) + 2 && i < pages) { %>
                            <li class="page-item disabled">
                              <a class="page-link" href="#">...</a>
                            </li>
                        <% } %>
                        <% } %>
                        
                        <!-- LAST ITEM -->
                        <% if(current == pages) { %> 
                      
                            <li class="page-item disabled">
                              <a class="page-link" href="#">
                                Last
                              </a>
                            </li>
                        <% } else { %>
                        
                            <li class="page-item">
                              <a class="page-link" href="/home/news/<%= pages %>/">
                                Last
                              </a>
                            </li>
                        <% } %>
                      </ul>
                    </nav>
                    <% } %>
                  </div>
                </div>
        </div>
      
    </section>
    <!-- Featured Section End -->

